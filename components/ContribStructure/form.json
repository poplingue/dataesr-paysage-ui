[
  {
    "form": [
      {
        "title": "Général",
        "subObject": "general",
        "content": [
          {
            "title": "Date de création",
            "type": "date",
            "validatorId": "creationDate"
          },
          {
            "title": "Texte officiel de création",
            "type": "text",
            "validatorId": "creationOfficialDocumentId",
            "dependency": {
              "validatorIds": ["creationReason"],
              "action": "hidden",
              "rule": "emptiness"
            }
          },
          {
            "title": "Motif de création",
            "type": "text",
            "validatorId": "creationReason"
          },
          {
            "title": "Description Fr",
            "type": "text",
            "validatorId": "descriptionFr"
          },
          {
            "title": "Description En",
            "type": "text",
            "validatorId": "descriptionEn"
          },
          {
            "title": "Texte officiel de fermeture",
            "type": "text",
            "validatorId": "closureOfficialDocumentId",
            "dependency": {
              "validatorIds": ["closureDate", "closureReason"],
              "action": "hidden",
              "rule": "emptiness"
            }
          },
          {
            "title": "Date de fermeture",
            "type": "date",
            "validatorId": "closureDate"
          },
          {
            "title": "Motif de fermeture",
            "type": "text",
            "validatorId": "closureReason"
          }
        ]
      },
      {
        "title": "Noms",
        "subObject": "names",
        "infinite": true,
        "content": [
          {
            "title": "Nom officiel",
            "type": "text",
            "validatorId": "officialName"
          },
          {
            "title": "Nom usuel",
            "type": "text",
            "validatorId": "usualName",
            "dependency": {
              "validatorIds": ["article", "acronymLocal", "shortName", "brandName", "otherNames"],
              "rule": "validator",
              "action": "hidden"
            }
          },
          {
            "title": "Article du nom usuel",
            "type": "select",
            "staticValues": [
              "à",
              "à l'",
              "à la",
              "dans le",
              "dans les",
              "aux",
              "au"
            ],
            "validatorId": "article"
          },
          {
            "title": "Acronyme",
            "type": "text",
            "validatorId": "acronymLocal"
          },
          {
            "title": "ShortName",
            "type": "text",
            "validatorId": "shortName"
          },
          {
            "title": "BrandName",
            "type": "text",
            "validatorId": "brandName"
          },
          {
            "title": "Acronyme français",
            "type": "text",
            "validatorId": "acronymFr"
          },
          {
            "title": "Nom anglais",
            "type": "text",
            "validatorId": "nameEn",
            "dependency": {
              "validatorIds": ["acronymEn"],
              "action": "hidden",
              "rule": "emptiness"
            }
          },
          {
            "title": "Acronyme anglais",
            "type": "text",
            "validatorId": "acronymEn"
          },
          {
            "title": "Autre nom",
            "type": "text",
            "infinite": true,
            "validatorId": "otherNames"
          },
          {
            "title": "Date de début",
            "type": "date",
            "validatorId": "startDate"
          },
          {
            "title": "Date de fin",
            "type": "date",
            "validatorId": "endDate"
          },
          {
            "title": "Commentaire",
            "type": "text",
            "validatorId": "comment"
          }
        ]
      },
      {
        "title": "Localisations",
        "subObject": "localisations",
        "infinite": true,
        "content": [
          {
            "title": "Code de la commune",
            "type": "text",
            "hint": "Code d'identification de la commune (code INSEE pour la France)",
            "validatorId": "cityId"
          },
          {
            "title": "Adresse",
            "type": "text",
            "suggest": true,
            "validatorId": "address",
            "dependency": {
              "validatorIds": ["postOfficeBoxNumber", "distributionStatement", "place", "country"],
              "rule": "emptiness",
              "action": "hidden"
            }
          },
          {
            "title": "Ville",
            "type": "text",
            "suggest": true,
            "validatorId": "locality"
          },
          {
            "title": "Code postal",
            "type": "text",
            "validatorId": "postalCode"
          },
          {
            "title": "Lieu-dit",
            "type": "text",
            "validatorId": "place"
          },
          {
            "title": "Mention de distribution",
            "type": "text",
            "hint": "bâtiment, tour, étage",
            "validatorId": "distributionStatement"
          },
          {
            "title": "Pays",
            "type": "text",
            "validatorId": "country"
          },
          {
            "title": "Boîte postale",
            "type": "text",
            "validatorId": "postOfficeBoxNumber"
          },
          {
            "title": "Téléphone",
            "type": "text",
            "validatorId": "telephone"
          },
          {
            "title": "Géolocalisation",
            "type": "group",
            "validatorId": "coordinates",
            "content": [
              {
                "title": "Longitude",
                "type": "text",
                "validatorId": "lng"
              },
              {
                "title": "Latitude",
                "type": "text",
                "validatorId": "lat"
              }
            ]
          },
          {
            "title": "Date de début",
            "type": "date",
            "validatorId": "startDate"
          },
          {
            "title": "Date de fin",
            "type": "date",
            "validatorId": "endDate"
          }
        ]
      },
      {
        "title": "Liens web",
        "subObject": "weblinks",
        "infinite": true,
        "content": [
          {
            "title": "Type",
            "type": "text",
            "validatorId": "type"
          },
          {
            "title": "Url",
            "type": "text",
            "validatorId": "url",
            "dependency": {
              "validatorIds": ["language"],
              "action": "hidden",
              "rule": "emptiness"
            }
          },
          {
            "title": "Langage",
            "type": "text",
            "validatorId": "language"
          }
        ]
      },
      {
        "title": "Réseaux sociaux",
        "subObject": "socials",
        "infinite": true,
        "content": [
          {
            "title": "Type",
            "type": "select",
            "validatorId": "type",
            "staticValues": [
              "Facebook",
              "Twitter",
              "Linkedin"
            ]
          },
          {
            "title": "Compte",
            "type": "text",
            "hint": "Identifiant du compte ou url",
            "validatorId": "account"
          }
        ]
      },
      {
        "title": "Liens Paysage",
        "subObject": "links",
        "content": [
          {
            "title": "Lier à une catégorie",
            "type": "multiSearch",
            "validatorId": "category"
          }
        ]
      },
      {
        "title": "Identifiants",
        "subObject": "identifiers",
        "infinite": true,
        "content": [
          {
            "title": "Type",
            "type": "text",
            "validatorId": "type",
            "dependency": {
              "validatorIds": ["value"],
              "action": "hidden"
            }
          },
          {
            "title": "Valeur",
            "type": "text",
            "validatorId": "value"
          },
          {
            "title": "État",
            "type": "radio",
            "validatorId": "active",
            "defaultLabel": "Actif",
            "staticValues": [
              {
                "value": true,
                "labelValue": "Actif"
              },
              {
                "value": false,
                "labelValue": "Inactif"
              }
            ]
          }
        ]
      }
    ]
  }
]
