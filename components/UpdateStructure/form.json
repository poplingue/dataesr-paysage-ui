          },
          {
            "title": "BrandName",
            "type": "text",
            "validatorId": "brandName"
          },
          {
            "title": "Acronyme français",
            "type": "text",
            "validatorId": "acronymFr"
          },
          {
            "title": "Nom anglais",
            "type": "text",
            "validatorId": "nameEn",
